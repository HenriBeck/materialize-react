{
  "name": "materialize-react",
  "version": "0.3.4",
  "description": "React Components with materialize theme",
  "main": "lib/index.js",
  "files": [
    "lib/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "start-storybook": "start-storybook -p 9001",
    "build-storybook": "build-storybook -o docs/",
    "test": "NODE_ENV=\"test\" ava --no-cache",
    "test:coverage": "nyc yarn run test",
    "generate-report:html": "nyc report --reporter=html",
    "generate-report:lcov": "nyc report --reporter=text-lcov",
    "prebuild": "rimraf lib/",
    "build": "NODE_ENV=\"build\" babel src -d lib && yarn run build:flow",
    "build:flow": "flow-copy-source src/ lib/ --ignore **/*.spec.js --ignore **/stories.jsx",
    "lint": "eslint src/ .storybook/ tests/ --ext .js,.jsx",
    "flow": "flow check src/",
    "prepare": "yarn run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HenriBeck/materialize-react.git"
  },
  "author": "Henri Beck",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HenriBeck/materialize-react/issues"
  },
  "homepage": "https://github.com/HenriBeck/materialize-react#readme",
  "dependencies": {
    "clamp": "1.0.1",
    "jss": "9.8.1",
    "lodash.merge": " 4.6.1",
    "lodash.noop": "3.0.1",
    "polished": "1.9.2",
    "prop-types": "15.6.1",
    "randomstring": "1.1.5",
    "react-context-subscriber": "0.4.0",
    "react-event-listener": "0.5.9",
    "react-get-not-declared-props": "1.0.0",
    "react-jss": "8.4.0",
    "theming": "1.3.0"
  },
  "peerDependencies": {
    "react": "^16.3.0",
    "react-dom": "^16.3.0"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.47",
    "@babel/core": "7.0.0-beta.47",
    "@babel/plugin-proposal-class-properties": "7.0.0-beta.47",
    "@babel/plugin-proposal-export-default-from": "7.0.0-beta.47",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0-beta.47",
    "@babel/preset-env": "7.0.0-beta.47",
    "@babel/preset-flow": "7.0.0-beta.47",
    "@babel/preset-react": "7.0.0-beta.47",
    "@babel/register": "7.0.0-beta.47",
    "@storybook/addon-actions": "4.0.0-alpha.8",
    "@storybook/addon-info": "4.0.0-alpha.8",
    "@storybook/addon-knobs": "4.0.0-alpha.8",
    "@storybook/addon-options": "4.0.0-alpha.8",
    "@storybook/addons": "4.0.0-alpha.8",
    "@storybook/react": "4.0.0-alpha.8",
    "ava": "1.0.0-beta.4",
    "babel-loader": "8.0.0-beta.3",
    "browser-env": "3.2.5",
    "css-loader": "0.28.11",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "4.19.1",
    "eslint-config-henribeck": "4.19.6",
    "flow-bin": "0.73.0",
    "flow-copy-source": "1.3.0",
    "jss-preset-default": "4.5.0",
    "mdi": "2.2.43",
    "normalize.css": "8.0.0",
    "nyc": "11.8.0",
    "raf": "3.4.0",
    "react": "16.4.0",
    "react-aux": "1.1.0",
    "react-dom": "16.4.0",
    "react-test-renderer": "16.4.0",
    "rimraf": "2.6.2",
    "sinon": "5.0.10",
    "style-loader": "0.21.0",
    "why-did-you-update": "0.1.1"
  },
  "ava": {
    "files": [
      "src/**/*.spec.js"
    ],
    "require": [
      "raf/polyfill",
      "@babel/register",
      "./tests/browser-env.js",
      "./tests/react.js"
    ],
    "concurrency": 10
  },
  "nyc": {
    "extension": [
      ".jsx"
    ],
    "exclude": [
      "*.spec.js",
      "tests/*"
    ],
    "report-dir": "./.coverage"
  }
}
