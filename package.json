{
  "name": "materialize-react",
  "version": "0.1.0-alpha.1",
  "description": "React Components with materialize theme",
  "main": "lib/index.js",
  "options": {
    "eslint": "--ext .jsx,.js src/ .storybook/ tests/",
    "storybook": "-p 9001"
  },
  "scripts": {
    "start-storybook": "start-storybook $npm_package_options_storybook",
    "build-storybook": "build-storybook",
    "test": "NODE_ENV=\"test\" ava",
    "test:watch": "yarn run test -- --watch",
    "test:coverage": "nyc yarn run test",
    "build": "babel src --out-dir lib --ignore *.spec.js",
    "generate-report:html": "nyc report --reporter=html",
    "generate-report:lcov": "nyc report --reporter=text-lcov",
    "report-coverage": "yarn run generate-report:lcov | coveralls",
    "lint": "eslint $npm_package_options_eslint",
    "lint:fix": "yarn run lint -- --fix",
    "prepublish": "yarn run build",
    "precommit": "yarn run lint",
    "deploy-storybook": "storybook-to-ghpages"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HenriBeck/materialize-react.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HenriBeck/materialize-react/issues"
  },
  "homepage": "https://github.com/HenriBeck/materialize-react#readme",
  "dependencies": {
    "classnames": "2.2.5",
    "lodash.omit": "4.5.0",
    "lodash.pick": "4.4.0",
    "polished": "1.3.0",
    "prop-types": "15.5.10",
    "randomstring": "1.1.5",
    "react": "15.6.1",
    "react-get-not-declared-props": "0.2.0",
    "react-jss": "7.0.2",
    "warning": "3.0.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "3.1.9",
    "@storybook/addon-comments": "3.1.9",
    "@storybook/addon-info": "3.1.9",
    "@storybook/addon-knobs": "3.2.0-alpha.0",
    "@storybook/addon-options": "3.1.6",
    "@storybook/react": "3.1.9",
    "@tf2-pickup/eslint-config": "6.0.1",
    "ava": "0.21.0",
    "babel-cli": "6.24.1",
    "babel-plugin-transform-export-extensions": "6.22.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "babel-register": "6.24.1",
    "browser-env": "2.0.31",
    "coveralls": "2.13.1",
    "css-loader": "0.28.4",
    "delay": "2.0.0",
    "enzyme": "2.9.1",
    "eslint": "4.3.0",
    "husky": "0.14.3",
    "is_js": "0.9.0",
    "mdi": "2.0.46",
    "normalize.css": "7.0.0",
    "nyc": "11.0.3",
    "react-addons-test-utils": "15.6.0",
    "react-dom": "15.6.1",
    "react-test-renderer": "15.6.1",
    "sinon": "2.3.8",
    "style-loader": "0.18.2"
  },
  "ava": {
    "files": [
      "src/**/*.spec.js"
    ],
    "require": [
      "babel-register",
      "babel-polyfill",
      "./tests/helpers/browser-env.js",
      "./tests/helpers/element-animate.js",
      "./tests/helpers/react.js"
    ],
    "babel": {
      "babelrc": true
    },
    "concurrency": 5
  },
  "nyc": {
    "check-coverage": true,
    "extension": [
      ".jsx"
    ],
    "exclude": [
      "src/**/*.spec.js",
      "tests/**/*"
    ],
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "report-dir": "./.coverage"
  }
}
